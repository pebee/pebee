{
    "presets": [
        ["@babel/preset-env", { "modules": false, "targets": { "node": "current" } }],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
    ]
}